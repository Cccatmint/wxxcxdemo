.wrap {
  background-color: #F4F6FA;  
}
.banner {
  width: 750rpx;
  height: 524rpx;
}
.cykdy {
  font-family: Helvetica;
  font-size: 14px;
  color: #999999;
  letter-spacing: -0.34px;
  margin: 32rpx auto 16rpx 40rpx;
}
.yjjc {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 710rpx;
  height: 156rpx;
  margin-bottom: 40rpx;
  background-color: #fff;
  border-radius: 12rpx;
}
.yjjc_img {
  width: 92rpx;
  height: 92rpx;
  margin: 32rpx 20rpx;
}
.yjjc_info {
  display: flex;
  flex-direction: column;
  width: 300rpx;
  height: 80rpx;
}
.yjjc_info1,
.yjjc_info2 {
  position: relative;
  top: 12rpx;
  display: flex;
  justify-content: space-around;
}
.yjjc_name,
.yjjc_tel {
  font-family: Helvetica;
  font-size: 28rpx;
  color: #626161;
  letter-spacing: -0.68rpx;
}
.yjjc_company,
.yjjc_adress {
  font-family: Helvetica;
  font-size: 12px;
  color: #999999;
  letter-spacing: -0.29px;
}
.yjjc_btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 186rpx;
  height: 60rpx;
  margin-right: 20rpx;
  border-radius: 30rpx;
  background-color: #35BD00;
}
.yjjc_btn_img {
  width: 28rpx;
  height: 22rpx;
}
.yjjc_btn_text {
  font-family: Helvetica;
  font-size: 12px;
  color: #FFFFFF;
  margin-left: 10rpx;
  letter-spacing: -0.29px;
}
.wddd {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 710rpx;
  height: 156rpx;
  margin-bottom: 40rpx;
  background-color: #fff;
  border-radius: 12rpx;
}
.wddd_img {
  width: 150rpx;
  height: 150rpx;
  margin-left: 20rpx;
}
.wddd_btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 186rpx;
  height: 60rpx;
  margin-right: 20rpx;
  border-radius: 30rpx;
  background-color: #35BD00;
}
.wddd_btn_img {
  width: 28rpx;
  height: 22rpx;
}
.wddd_btn_text {
  font-family: Helvetica;
  font-size: 12px;
  color: #FFFFFF;
  margin-left: 10rpx;
  letter-spacing: -0.29px;
}